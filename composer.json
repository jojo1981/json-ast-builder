{
  "name": "jojo1981/json-ast-builder",
  "description": "Json AST builder",
  "keywords": [
    "json",
    "syntax",
    "semantic",
    "tokenize",
    "token",
    "parser",
    "lexer",
    "ast",
    "abstract",
    "tree",
    "traversal",
    "visitor"
  ],
  "type": "library",
  "license": "MIT",
  "readme": "README.md",
  "authors": [
    {
      "name": "Joost Nijhuis",
      "email": "jnijhuis81@gmail.com"
    }
  ],
  "prefer-stable": true,
  "minimum-stability": "dev",
  "require": {
    "php": "^8.0|^8.1|^8.2|^8.3|^8.4",
    "ext-ctype": "*"
  },
  "require-dev": {
    "dg/bypass-finals": "^1.9",
    "phpunit/phpunit": "^9.0"
  },
  "autoload": {
    "psr-4": {
      "Jojo1981\\JsonAstBuilder\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "tests\\Jojo1981\\JsonAstBuilder\\": "tests/"
    }
  },
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "scripts": {
    "test": [
      "./bin/phpunit"
    ]
  }
}
